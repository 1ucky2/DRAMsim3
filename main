close all
clear
clc

data = csvread('./build/ddr4-output-final_power_tempeature.csv', 1, 0);
pos = csvread('./build/ddr4-output-bank_position.csv', 1, 0); 

X = max(data(:,2));
Y = max(data(:,3));
Z = max(data(:,4));
fprintf('Floorplan: (%d, %d, %d)\n', X+1, Y+1, Z+1);

power = zeros(X+1, Y+1, Z+1);
temperature = power; 

for i = 1 : size(data, 1)
    power(data(i,2)+1, data(i,3)+1, data(i,4)) = data(i,5);
    temperature(data(i,2)+1, data(i,3)+1, data(i,4)) = data(i,6);
end

